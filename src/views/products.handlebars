<section class="section_products">
    <h1>Lista de productos</h1>
    <ul>
        {{#each productos }}
        <div class="conteinterProducts">
            <h2>{{title}}</h2>
            <div class="conteinterProductImg">
            <img class="productImg" src="{{img}}"></img>
            </div>
            <p>{{description}}</p>
            <p>Precio: ${{price}}</p>
            <p>Categoria: {{category}} </p>
            <button> Agregar al Carrito </button>
        </div>  
        {{/each}}
    </ul>
    <div class="prev-netxPage_buttons">
        {{#unless hasPrevPage}}
        <span class="prev_page_notEnabled">Atrás</span>
        {{/unless}}
        {{#if hasPrevPage}}
        <a href="/products?page={{prevPage}}">Atrás</a>
        {{/if}}
        <p>Pagina {{currentPage}} de {{totalPages}}</p>
        {{#if hasNextPage}}
        <a href="/products?page={{nextPage}}">Siguiente</a>
        {{/if}}
        {{#unless hasNextPage}}
        <span class="prev_page_notEnabled">Siguiente</span>
        {{/unless}}
    </div>
</section>